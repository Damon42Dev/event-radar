{
  "manifest_version": 3,
  "name": "Web Event to Calendar",
  "version": "1.0.0",
  "description": "提取网页事件并导入日历的 Chrome 扩展",
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:3000/*",
    "https://*/"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.js-Co584USk.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/index.js-Co584USk.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
